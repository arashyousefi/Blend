function (integer) main()
{
	integer a := 5;
	integer b := 3;
    do{
        b := 3;
        while(b > 0){
            write(b);
            if(b = 2){
                break;
            }
            b := b-1;
        }
        write(a);
        if(a = 2){
            break;
        }
        a := a-1;
    }while(a > 0)
}